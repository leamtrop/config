!Reload changes made to file:
!xrdb ~/.Xresources
!##################################################
!# Xft settings
!##################################################
Xft.dpi:                    92
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintfull

!##################################################
!# URxvt settings
!# checkout the look with: colortheme
!##################################################

!URxvt*termName:   rxvt-unicode-256color
URxvt.termName: rxvt-unicode-256color
!URxvt*locale:	true

!URxvt fonts:
URxvt.allow_bold    true
!URxvt*font: 	xft:Source Code Pro for Powerline:pixelsize=15,xft:Monospace:size=20:antialias=true:hinting=true
!URxvt*boldFont: xft:Source Code Pro for Powerline:bold:pixelsize=15,xft:Monospace:size=20:antialias=true
URxvt.font: xft:Monaco:bold:pixelsize=15,xft:zhHei:pixelsize=16
URxvt.boldFont: xft:Monaco:bold:pixelsize=15,xft:zhHei:pixelsize=16
URxvt.color12: rgb:5c/5c/ff
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False  

!Fix font space
URxvt.letterSpace: 0

!URxvt scrolling options:
URxvt*saveLines:            12000
!URxvt*scrollstyle:          plain
URxvt*scrollstyle:          rxvt
URxvt*scrollBar:            false

URxvt*urgentOnBell:         true
URxvt*cursorBlink:          true
URxvt*cursorUnderline:      false
URxvt*cursorColor:          #555
URxvt*cursorColor2:         #cee

!hide mouse pointer as soon as start typing
URxvt.pointerBlank: 		true
URxvt*pointerBlankDelay: 	1

!global colors that also work for URxvt start
!*fading: 40
*background:            #002b36
*foreground:            #839496
*fadeColor:             #002b36
*cursorColor:           #93a1a1
*pointerColorBackground:#586e75
*pointerColorForeground:#93a1a1

!! black dark/light
*color0:                #073642
*color8:                #002b36
!! red dark/light
*color1:                #dc322f
*color9:                #cb4b16
!! green dark/light
*color2:                #859900
*color10:               #586e75
!! yellow dark/light
*color3:                #b58900
*color11:               #657b83
!! blue dark/light
*color4:                #268bd2
*color12:               #839496
!! magenta dark/light
*color5:                #d33682
*color13:               #6c71c4
!! cyan dark/light
*color6:                #2aa198
*color14:               #93a1a1
!! white dark/light
*color7:                #eee8d5
*color15:               #fdf6e3

! color for URL
*colorUL :             #8dabcd
!global colors that also work for URxvt end

!URxvt extensions:
URxvt.perl-ext-common:      default,matcher,clipboard
URxvt.matcher.button:       1
URxvt.urlLauncher:          chromium

URxvt.keysym.C-Delete:      perl:matcher:last
URxvt.keysym.M-Delete:      perl:matcher:list

!disable printing from terminal
URxvt.print-pipe: "cat > /dev/null"

!光标闪烁
URxvt.cursorBlink: true
URxvt.saveLines: 10000
