[core]
    quotepath = false
    autocrlf = false
    preloadindex = true
    fscache = true
[color]
    ui = true
    diff = auto
    status = auto
    branch = auto
    interactive = auto
[alias]
    a = add
    b = branch
    ba = branch -a
    bd = branch -d
    bdd = branch -D
    br = branch -r
    co = checkout
    cob = checkout -b
    c = commit
    ca = commit -a
    cm = commit -m
    cl = clone
    cp = cherry-pick
    cpa = cherry-pick --abort
    cpc = cherry-pick --continue
    d = diff
    ds = diff --staged
    dc = diff --cached
    dk = diff --check
    f = fetch
    fo = fetch origin
    fu = fetch upstream
    g = grep -p
    l = log --oneline
    lg = log --color --graph --date=format:'%Y-%m-%d %H:%M:%S' --format=format:'%C(yellow)%h%Creset %s %C(bold blue)<%an> %Cgreen(%ad)%Cred%d%Creset' --abbrev-commit
    lg1 = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    m = merge
    ma = merge --abort
    mc = merge --continue
    ms = merge --skip
    ps = push
    pso = push origin
    psom = push origin master
    pl = pull
    plc = "!git pull origin $(git rev-parse --abbrev-ref HEAD)"
    plo = pull origin
    plom = pull origin master
    plu = pull upstream
    plum = pull upstream master
    rb = rebase
    rba = rebase --abort
    rbc = rebase --continue
    rbi = rebase --interactive
    rbs = rebase --skip
    rs = reset
    rh = reset HEAD
    rss = reset --soft
    rsh = reset --hard
    rm = remote
    rv = revert
    st = status
[gc]
    auto = 256
[push]
    default = current
